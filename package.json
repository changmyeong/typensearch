{
  "name": "typensearch",
  "version": "2.0.3",
  "description": "Awesome OpenSearch model using TypeScript classes.",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "test": "jest --testPathIgnorePatterns=src/__tests__/e2e/",
    "test:watch": "jest --watch --testPathIgnorePatterns=src/__tests__/e2e/",
    "test:coverage": "jest --coverage --testPathIgnorePatterns=src/__tests__/e2e/",
    "test:e2e": "jest src/__tests__/e2e --testMatch=**/*.test.ts",
    "test:e2e:watch": "jest src/__tests__/e2e --watch --testMatch=**/*.test.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/changmyeong/typensearch.git"
  },
  "keywords": [
    "opensearch",
    "schema",
    "database",
    "orm",
    "odm",
    "aws",
    "amazon",
    "query",
    "scan",
    "object",
    "storage",
    "decorator",
    "mapping",
    "elasticsearch",
    "typescript"
  ],
  "author": {
    "name": "changmyeong",
    "email": "changmyeong.baek@gmail.com",
    "url": "https://github.com/changmyeong"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/changmyeong/typensearch/issues"
  },
  "homepage": "https://github.com/changmyeong/typensearch#readme",
  "dependencies": {
    "@opensearch-project/opensearch": "^2.2.1",
    "@types/uuid": "^10.0.0",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "@types/node": "^18.16.3",
    "jest": "^29.7.0",
    "ts-jest": "^29.3.0",
    "typescript": "^5.0.4"
  }
}
